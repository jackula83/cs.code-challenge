{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "jackula83/allocate-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "jackula83/allocate-server",
      "hostname": "api",
      "essential": false,
      "memory": 256
    },
    {
      "name": "nginx",
      "image": "jackula83/allocate-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "server",
        "client"
      ],
      "memory": 128
    }
  ]
}